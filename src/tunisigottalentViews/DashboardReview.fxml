<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tunisigottalentViews.DashboardReviewController">
    <stylesheets>
        <URL value="@dashboardreview.css" />
    </stylesheets>
   <children>
      <TableView fx:id="tabrev" layoutX="127.0" layoutY="113.0" prefHeight="173.0" prefWidth="457.0">
         <columns>
            <TableColumn fx:id="id" prefWidth="116.0" text="ID" />
            <TableColumn fx:id="category" prefWidth="71.0" text="Category" />
            <TableColumn fx:id="title" prefWidth="121.0" text="Title" />
            <TableColumn fx:id="rating" prefWidth="116.0" text="Rating" />
            <TableColumn fx:id="content" prefWidth="116.0" text="Content" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="refresh" layoutX="463.0" layoutY="323.0" mnemonicParsing="false" onAction="#Actualiser" text="Refresh" />
      <Button fx:id="delete" layoutX="367.0" layoutY="323.0" mnemonicParsing="false" onAction="#Suppimer" text="Delete" />
      <Label layoutX="286.0" layoutY="55.0" text="Your review's list" textAlignment="CENTER" />
      <Button fx:id="cancel" layoutX="260.0" layoutY="323.0" mnemonicParsing="false" onAction="#retour" text="Return" />
   </children>
</AnchorPane>
