<AnchorPane id="AnchorPane" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.ShoppingCartController">
   <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#6356e5" height="720.0" layoutY="2.0" stroke="TRANSPARENT" strokeType="INSIDE" width="400.0">
         <effect>
            <DropShadow />
         </effect>
      </Rectangle>
      <TableView fx:id="tableProduct" layoutX="545.0" layoutY="98.0" prefHeight="529.0" prefWidth="622.0" stylesheets="@shoppingcart.css">
         <columns>
            <TableColumn fx:id="table_id" prefWidth="42.0" text="id" />
            <TableColumn fx:id="table_productname" prefWidth="208.0" text="Product Name" />
            <TableColumn fx:id="table_image" prefWidth="254.0" text="Image" />
            <TableColumn fx:id="table_stock" prefWidth="59.0" text="Stock" />
            <TableColumn fx:id="table_price" prefWidth="58.0" text="Price" />
         </columns>
      </TableView>
      <Label fx:id="product_label" layoutX="737.0" layoutY="14.0" prefHeight="64.0" prefWidth="237.0" text="Shopping Cart" textAlignment="CENTER" textFill="#55b3f3">
         <font>
            <Font name="System Bold" size="34.0" />
