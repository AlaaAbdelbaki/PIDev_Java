<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" fx:id="rootPane" prefHeight="750.0" prefWidth="1600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tunisiagottalent.UI.User.ProfileController">
    <stylesheets>
        <URL value="@profile.css" />
    </stylesheets>
   <children>
      <ImageView fx:id="profilePic" fitHeight="194.0" fitWidth="194.0" layoutX="146.0" layoutY="74.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@img/unknown.jpg" />
         </image>
      </ImageView>
      <Label fx:id="username_profile" layoutX="159.0" layoutY="312.0" prefHeight="27.0" prefWidth="445.0" styleClass="userinfo" text="username" />
      <Label fx:id="nameLastName" layoutX="143.0" layoutY="384.0" prefHeight="36.0" prefWidth="267.0" styleClass="userinfo" text="firstLastName" />
      <Label fx:id="bio" alignment="TOP_LEFT" layoutX="139.0" layoutY="487.0" prefHeight="118.0" prefWidth="162.0" styleClass="bio" text="Bio" wrapText="true" />
      <ScrollPane fx:id="videos_Container" layoutX="468.0" layoutY="156.0" prefHeight="501.0" prefWidth="880.0" stylesheets="@../Competitions/scroll.css" />
      <Button layoutX="1413.0" layoutY="324.0" mnemonicParsing="false" onAction="#addVideo" text="Add Video" />
      <Button layoutX="1410.0" layoutY="390.0" mnemonicParsing="false" onAction="#editProfile" text="Edit Profile" />
   </children>
</AnchorPane>
