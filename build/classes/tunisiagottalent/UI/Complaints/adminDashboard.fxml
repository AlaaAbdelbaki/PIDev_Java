<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="750.0" prefWidth="1600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tunisiagottalent.UI.Complaints.AdminDashboardController">
    <stylesheets>
        <URL value="@admindashboard.css" />
    </stylesheets>
   <children>
      <TabPane fx:id="tab1" layoutX="350.0" layoutY="12.0" prefHeight="720.0" prefWidth="1181.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="ComplaintList">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="440.0" prefWidth="1025.0">
                     <children>
                        <TableView fx:id="tab" layoutX="6.0" layoutY="42.0" prefHeight="200.0" prefWidth="852.0">
                          <columns>
                            <TableColumn fx:id="colid" prefWidth="66.0" text="ID" />
                              <TableColumn fx:id="colsubj" prefWidth="142.0" text="Subject" />
                              <TableColumn fx:id="colcont" prefWidth="182.0" text="Content" />
                              <TableColumn fx:id="usernamec" prefWidth="109.0" text="Username" />
                          </columns>
                        </TableView>
                        <Button fx:id="delete" layoutX="385.0" layoutY="302.0" mnemonicParsing="false" onAction="#Remove" text="Delete" />
                        <Button fx:id="Refresh" layoutX="267.0" layoutY="302.0" mnemonicParsing="false" onAction="#actualiser" text="Refresh" />
                        <TextField fx:id="search" layoutX="261.0" layoutY="8.0" onAction="#SearchSubject" />
                        <Label layoutX="195.0" layoutY="12.0" text="Search" />
                        <TextArea fx:id="areaText" layoutX="325.0" layoutY="417.0" prefHeight="89.0" prefWidth="393.0" />
                        <Button fx:id="mail" layoutX="582.0" layoutY="534.0" mnemonicParsing="false" onAction="#respond" text="Respond" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="ReviewList">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="314.0" prefWidth="525.0">
                     <children>
                        <TableView fx:id="tabrec" layoutX="42.0" layoutY="79.0" prefHeight="246.0" prefWidth="1080.0">
                           <columns>
                              <TableColumn fx:id="id" prefWidth="66.0" text="ID" />
                              <TableColumn fx:id="username" prefWidth="72.0" text="ID_user" />
                              <TableColumn fx:id="colcat" prefWidth="83.0" text="Category" />
                              <TableColumn fx:id="rate" prefWidth="66.0" text="Rating" />
                              <TableColumn fx:id="coltitre" prefWidth="75.0" text="Title" />
                              <TableColumn fx:id="con" prefWidth="75.0" text="Content" />
                           </columns>
                           <columnResizePolicy>
                          
                           </columnResizePolicy>
                        </TableView>
                        <TextField fx:id="rech" layoutX="262.0" layoutY="14.0" onAction="#SearchRating" />
                        <Label layoutX="199.0" layoutY="18.0" text="Search" />
                        <Button fx:id="ActualR" layoutX="222.0" layoutY="342.0" mnemonicParsing="false" onAction="#actualiserRev" text="Refresh" />
                        <Button fx:id="deleteR" layoutX="334.0" layoutY="342.0" mnemonicParsing="false" onAction="#RemoveReview" text="Delete" />
                        <Button fx:id="stat" layoutX="433.0" layoutY="342.0" mnemonicParsing="false" onAction="#Stat" text="Stat" />
                     </children></AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
